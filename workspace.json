{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "generators": { "@nrwl/react": { "application": { "babel": true } } },
  "projects": {
    "ui-button": {
      "root": "libs/ui-button",
      "sourceRoot": "libs/ui-button/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui-button/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui-button"],
          "options": {
            "jestConfig": "libs/ui-button/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": { "configFolder": "libs/ui-button/.storybook" }
          },
          "configurations": { "ci": { "quiet": true } }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/ui-button",
            "config": { "configFolder": "libs/ui-button/.storybook" }
          },
          "configurations": { "ci": { "quiet": true } }
        }
      }
    },
    "ui-button-e2e": {
      "root": "apps/ui-button-e2e",
      "sourceRoot": "apps/ui-button-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui-button-e2e/cypress.json",
            "tsConfig": "apps/ui-button-e2e/tsconfig.e2e.json",
            "devServerTarget": "ui-button:storybook"
          },
          "configurations": {
            "ci": { "devServerTarget": "ui-button:storybook:ci" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/ui-button-e2e/**/*.{js,ts}"] }
        }
      }
    }
  }
}
